# sleigh specification file for WDC 65816


define endian=little;
define alignment=1;

define space RAM       type=ram_space       size=3  default;
define space register  type=register_space  size=1;

define register offset=0x00  size=3  [ PC_FULL ];
define register offset=0x00  size=2  [ PC ];
define register offset=0x02  size=1  [ PBR ];

define register offset=0x10  size=2  [ C   X          Y          DP      SP ];
define register offset=0x10  size=1  [ A B XLow XHigh YLow YHigh DPLow _ SPLow SPHigh ];
define register offset=0x20  size=1  [ DBR ];

# Status flags.
define register offset=0x30  size=1  [ NF VF DF IF ZF CF BF ];
define register offset=0x40  size=4  contextreg;
define context contextreg
    ctx_MF=(0,0)
    ctx_XF=(1,1)
    # DF removed from slot 2
    ctx_EF=(3,3)
;
# Unfortunately the ctx_xx fields can't be used at all in P-code macros, and
# are a pain to use in a semantic section.
# These preprocessor macros work around this and allow for simpler manipulation
# of status flags (bit range access/assignments).
@define MF "contextreg[0,1]"
@define XF "contextreg[1,1]"
# @define DF "contextreg[2,1]"
@define EF "contextreg[3,1]"


define token opbyte (8)
   op       = (0,7)

   aaa      = (5,7)
   bbb      = (2,4)
   cc       = (0,1)
   dd       = (2,3)
;

define token data8 (8)
   imm8     = (0,7)
   rel8      = (0,7) signed
;

define token movearg (8)
    srcbk    = (0,7)
    destbk   = (0,7)
;

define token repsep (8)
    op_flags = (0,7)
    op_nf = (7,7)
    op_vf = (6,6)
    op_mf = (5,5)
    op_xf = (4,4)
    op_df = (3,3)
    op_if = (2,2)
    op_zf = (1,1)
    op_cf = (0,0)
;

define token data16 (16)
    imm16 = (0,15)
    rel16 = (0,15) signed
;

define token data24 (24)
    imm24 = (0,23)
;


# TODO: deal with 65802/65816 in native/emulation modes?
# TODO: make sure we're setting flags for all instructions.
# TODO: check instructions' use of DP addressing modes with or without special emulation behaviour.


##################################################################
# Specify addressing modes for the different instruction groups.
##################################################################

@include "658xx_memaccess.sinc"

# Two subsets of the addressing modes for the primary group instructions
# (ADC, AND, CMP, EOR, LDA, ORA, SBC, STA).
#
# PRIMARY_OP_NO_IMM_READn is the whole subset except for immediate mode (can't STA to an immediate).
#
PRIMARY_OP_NO_IMM_READ8: MemAbsRead8    is bbb=3 & cc=1; MemAbsRead8  { export MemAbsRead8; }
PRIMARY_OP_NO_IMM_READ8: MemAbsLongRead8    is bbb=3 & cc=3; MemAbsLongRead8  { export MemAbsLongRead8; }
PRIMARY_OP_NO_IMM_READ8: MemDPRead8    is bbb=1 & cc=1; MemDPRead8  { export MemDPRead8; }
PRIMARY_OP_NO_IMM_READ8: MemDPIndRead8    is bbb=4 & cc=2; MemDPIndRead8  { export MemDPIndRead8; }
PRIMARY_OP_NO_IMM_READ8: MemDPIndLongRead8    is bbb=1 & cc=3; MemDPIndLongRead8  { export MemDPIndLongRead8; }
PRIMARY_OP_NO_IMM_READ8: MemAbsIdxXRead8    is bbb=7 & cc=1; MemAbsIdxXRead8  { export MemAbsIdxXRead8; }
PRIMARY_OP_NO_IMM_READ8: MemAbsLongIdxXRead8    is bbb=7 & cc=3; MemAbsLongIdxXRead8  { export MemAbsLongIdxXRead8; }
PRIMARY_OP_NO_IMM_READ8: MemAbsIdxYRead8    is bbb=6 & cc=1; MemAbsIdxYRead8  { export MemAbsIdxYRead8; }
PRIMARY_OP_NO_IMM_READ8: MemDPIdxXRead8    is bbb=5 & cc=1; MemDPIdxXRead8  { export MemDPIdxXRead8; }
PRIMARY_OP_NO_IMM_READ8: MemDPIdxIndRead8    is bbb=0 & cc=1; MemDPIdxIndRead8  { export MemDPIdxIndRead8; }
PRIMARY_OP_NO_IMM_READ8: MemDPIndIdxRead8    is bbb=4 & cc=1; MemDPIndIdxRead8  { export MemDPIndIdxRead8; }
PRIMARY_OP_NO_IMM_READ8: MemDPIndLongIdxRead8    is bbb=5 & cc=3; MemDPIndLongIdxRead8  { export MemDPIndLongIdxRead8; }
PRIMARY_OP_NO_IMM_READ8: MemSRRead8    is bbb=0 & cc=3; MemSRRead8  { export MemSRRead8; }
PRIMARY_OP_NO_IMM_READ8: MemSRIndRead8    is bbb=4 & cc=3; MemSRIndRead8  { export MemSRIndRead8; }
#
PRIMARY_OP_NO_IMM_READ16: MemAbsRead16    is bbb=3 & cc=1; MemAbsRead16  { export MemAbsRead16; }
PRIMARY_OP_NO_IMM_READ16: MemAbsLongRead16    is bbb=3 & cc=3; MemAbsLongRead16  { export MemAbsLongRead16; }
PRIMARY_OP_NO_IMM_READ16: MemDPRead16    is bbb=1 & cc=1; MemDPRead16  { export MemDPRead16; }
PRIMARY_OP_NO_IMM_READ16: MemDPIndRead16    is bbb=4 & cc=2; MemDPIndRead16  { export MemDPIndRead16; }
PRIMARY_OP_NO_IMM_READ16: MemDPIndLongRead16    is bbb=1 & cc=3; MemDPIndLongRead16  { export MemDPIndLongRead16; }
PRIMARY_OP_NO_IMM_READ16: MemAbsIdxXRead16    is bbb=7 & cc=1; MemAbsIdxXRead16  { export MemAbsIdxXRead16; }
PRIMARY_OP_NO_IMM_READ16: MemAbsLongIdxXRead16    is bbb=7 & cc=3; MemAbsLongIdxXRead16  { export MemAbsLongIdxXRead16; }
PRIMARY_OP_NO_IMM_READ16: MemAbsIdxYRead16    is bbb=6 & cc=1; MemAbsIdxYRead16  { export MemAbsIdxYRead16; }
PRIMARY_OP_NO_IMM_READ16: MemDPIdxXRead16    is bbb=5 & cc=1; MemDPIdxXRead16  { export MemDPIdxXRead16; }
PRIMARY_OP_NO_IMM_READ16: MemDPIdxIndRead16    is bbb=0 & cc=1; MemDPIdxIndRead16  { export MemDPIdxIndRead16; }
PRIMARY_OP_NO_IMM_READ16: MemDPIndIdxRead16    is bbb=4 & cc=1; MemDPIndIdxRead16  { export MemDPIndIdxRead16; }
PRIMARY_OP_NO_IMM_READ16: MemDPIndLongIdxRead16    is bbb=5 & cc=3; MemDPIndLongIdxRead16  { export MemDPIndLongIdxRead16; }
PRIMARY_OP_NO_IMM_READ16: MemSRRead16    is bbb=0 & cc=3; MemSRRead16  { export MemSRRead16; }
PRIMARY_OP_NO_IMM_READ16: MemSRIndRead16    is bbb=4 & cc=3; MemSRIndRead16  { export MemSRIndRead16; }
#
#
# PRIMARY_OP_READn is the whole subset including immediate mode.
#
PRIMARY_OP_READ8: PRIMARY_OP_NO_IMM_READ8    is PRIMARY_OP_NO_IMM_READ8  { export PRIMARY_OP_NO_IMM_READ8; }
PRIMARY_OP_READ8: MemImmRead8    is bbb=2 & cc=1; MemImmRead8  { export MemImmRead8; }
#
PRIMARY_OP_READ16: PRIMARY_OP_NO_IMM_READ16    is PRIMARY_OP_NO_IMM_READ16  { export PRIMARY_OP_NO_IMM_READ16; }
PRIMARY_OP_READ16: MemImmRead16    is bbb=2 & cc=1; MemImmRead16  { export MemImmRead16; }


# Subsets of the addressing modes for some secondary instructions (most of the remaining
# instructions that aren't branches/jumps and don't use implied addressing).
#
SEC_OP_COMMON_READ8: MemAbsRead8    is bbb=3; MemAbsRead8  { export MemAbsRead8; }
SEC_OP_COMMON_READ8: MemDPRead8    is bbb=1; MemDPRead8  { export MemDPRead8; }
#
SEC_OP_COMMON_READ16: MemAbsRead16    is bbb=3; MemAbsRead16  { export MemAbsRead16; }
SEC_OP_COMMON_READ16: MemDPRead16    is bbb=1; MemDPRead16  { export MemDPRead16; }
#
SEC_OP_X_READ8: SEC_OP_COMMON_READ8    is SEC_OP_COMMON_READ8  { export SEC_OP_COMMON_READ8; }
SEC_OP_X_READ8: MemAbsIdxXRead8    is bbb=7; MemAbsIdxXRead8  { export MemAbsIdxXRead8; }
SEC_OP_X_READ8: MemDPIdxXRead8    is bbb=5; MemDPIdxXRead8  { export MemDPIdxXRead8; }
#
SEC_OP_X_READ16: SEC_OP_COMMON_READ16    is SEC_OP_COMMON_READ16  { export SEC_OP_COMMON_READ16; }
SEC_OP_X_READ16: MemAbsIdxXRead16    is bbb=7; MemAbsIdxXRead16  { export MemAbsIdxXRead16; }
SEC_OP_X_READ16: MemDPIdxXRead16    is bbb=5; MemDPIdxXRead16  { export MemDPIdxXRead16; }
#
SEC_OP_Y_READ8: SEC_OP_COMMON_READ8    is SEC_OP_COMMON_READ8  { export SEC_OP_COMMON_READ8; }
SEC_OP_Y_READ8: MemAbsIdxYRead8    is bbb=7; MemAbsIdxYRead8  { export MemAbsIdxYRead8; }
SEC_OP_Y_READ8: MemDPIdxYRead8    is bbb=5; MemDPIdxYRead8  { export MemDPIdxYRead8; }
#
SEC_OP_Y_READ16: SEC_OP_COMMON_READ16    is SEC_OP_COMMON_READ16  { export SEC_OP_COMMON_READ16; }
SEC_OP_Y_READ16: MemAbsIdxYRead16    is bbb=7; MemAbsIdxYRead16  { export MemAbsIdxYRead16; }
SEC_OP_Y_READ16: MemDPIdxYRead16    is bbb=5; MemDPIdxYRead16  { export MemDPIdxYRead16; }

#### SHIFT_OP: SEC_OP_X    is SEC_OP_X  { export SEC_OP_X; }
#### SHIFT_OP: ADDR_ACC    is bbb=2; ADDR_ACC  { export ADDR_ACC; }


##################################################################
# Specify instruction patterns and semantics.
##################################################################

# ADC 8 bit.
:ADC PRIMARY_OP_READ8    is (ctx_MF=1 &
    (op=0x69 | op=0x6d | op=0x6f | op=0x65 | op=0x72 | op=0x67 | op=0x7d |
     op=0x7f | op=0x79 | op=0x75 | op=0x61 | op=0x71 | op=0x77 | op=0x63 |
     op=0x73)) ... & PRIMARY_OP_READ8
{
    local is_dec:1 = DF == 1;
    local not_dec:1 = DF == 0;
    local do_skip_dec_adjust:1;
    local data:1 = PRIMARY_OP_READ8;
    local result:4;

    if (is_dec) goto <decimal>;

    <binary>
    result = zext(A) + zext(data) + zext(CF);
    goto <flags>;

    <decimal>
    # Units.
    result = (zext(A) & 0x0f) + (zext(data) & 0x0f) + zext(CF);
    do_skip_dec_adjust = (result s<= 0x09);
    if (do_skip_dec_adjust) goto <skip_dec_adjust>;
    result = result + 0x06;
    <skip_dec_adjust>
    CF = (result s> 0x0f);

    # Tens.
    result = (zext(A) & 0xf0) + (zext(data) & 0xf0) + (zext(CF) << 4) + (result & 0x0f);

    <flags>
    VF = (~zext(A ^ data) & (zext(A) ^ result) & 0x80) != 0;
    # Do this ridiculous dance to properly infer sizes, otherwise we get
    # "Output must be a boolean" errors...
    local too_small:1 = result s<= 0x9f;
    do_skip_dec_adjust = (not_dec || too_small);
    if (do_skip_dec_adjust) goto <skip_dec_adjust_flags>;
    result = result + 0x60;
    <skip_dec_adjust_flags>
    CF = result s> 0xFF;
    local result8:1 = result:1;
    flag8_NZ(result8);
    A = result8;
}

# ADC 16 bit.
:ADC PRIMARY_OP_READ16    is (ctx_MF=0 &
    (op=0x69 | op=0x6d | op=0x6f | op=0x65 | op=0x72 | op=0x67 | op=0x7d |
     op=0x7f | op=0x79 | op=0x75 | op=0x61 | op=0x71 | op=0x77 | op=0x63 |
     op=0x73)) ... & PRIMARY_OP_READ16
{
    local is_dec:1 = DF == 1;
    local not_dec:1 = DF == 0;
    local do_skip_dec_adjust:1;
    local data:2 = PRIMARY_OP_READ16;
    local result:4;

    if (is_dec) goto <decimal>;

    <binary>
    result = zext(C) + zext(data) + zext(CF);
    goto <flags>;

    <decimal>
    # Units.
    result = (zext(C) & 0x0f) + (zext(data) & 0x0f) + zext(CF);
    do_skip_dec_adjust = (result s<= 0x09);
    if (do_skip_dec_adjust) goto <skip_dec_adjust_1>;
    result = result + 0x06;
    <skip_dec_adjust_1>
    CF = result s> 0x0f;

    # Tens.
    result = (zext(C) & 0xf0) + (zext(data) & 0xf0) + (zext(CF) << 4) + (result & 0x0f);
    do_skip_dec_adjust = (result s<= 0x9f);
    if (do_skip_dec_adjust) goto <skip_dec_adjust_2>;
    result = result + 0x60;
    <skip_dec_adjust_2>
    CF = result s> 0xff;

    # Hundreds.
    result = (zext(C) & 0x0f00) + (zext(data) & 0x0f00) + (zext(CF) << 8) + (result & 0xff);
    do_skip_dec_adjust = (result s<= 0x09ff);
    if (do_skip_dec_adjust) goto <skip_dec_adjust_3>;
    result = result + 0x0600;
    <skip_dec_adjust_3>
    CF = result s> 0x0fff;

    # Thousands.
    result = (zext(C) & 0xf000) + (zext(data) & 0xf000) + (zext(CF) << 12) + (result & 0x0fff);

    <flags>
    VF = ((~zext(C ^ data)) & (zext(C) ^ result) & 0x8000) != 0;
    local too_small:1 = (result s<= 0x9fff);
    do_skip_dec_adjust = (not_dec || too_small);
    if (do_skip_dec_adjust) goto <skip_dec_adjust_flags>;
    result = result + 0x6000;
    <skip_dec_adjust_flags>
    CF = result s> 0xFFFF;
    local result16:2 = result:2;
    flag16_NZ(result16);
    C = result16;
}

# AND 8 bit.
:AND PRIMARY_OP_READ8    is (ctx_MF=1 &
    (op=0x29 | op=0x2d | op=0x2f | op=0x25 | op=0x32 | op=0x27 | op=0x3d |
     op=0x3f | op=0x39 | op=0x35 | op=0x21 | op=0x31 | op=0x37 | op=0x23 |
     op=0x33)) ... & PRIMARY_OP_READ8
{
    local data:1 = PRIMARY_OP_READ8;

    A = A & data;
    flag8_NZ(A);
}

# AND 16 bit.
:AND PRIMARY_OP_READ16    is (ctx_MF=0 &
    (op=0x29 | op=0x2d | op=0x2f | op=0x25 | op=0x32 | op=0x27 | op=0x3d |
     op=0x3f | op=0x39 | op=0x35 | op=0x21 | op=0x31 | op=0x37 | op=0x23 |
     op=0x33)) ... & PRIMARY_OP_READ16
{
    local data:2 = PRIMARY_OP_READ16;

    C = C & data;
    flag16_NZ(C);
}

# ASL 8 bit.
:ASL SHIFT_OP    is (ctx_MF=1 &
    (op=0x0a | op=0x0e | op=0x06 | op=0x1e | op=0x16)) ... & SHIFT_OP
{
    local data:1 = SHIFT_OP:1;
    local result:1 = data << 1;

    SHIFT_OP[0,8] = result;
    flag8_NZ(result);
    CF = data[7,1];
}

# ASL 16 bit.
:ASL SHIFT_OP    is (ctx_MF=0 &
    (op=0x0a | op=0x0e | op=0x06 | op=0x1e | op=0x16)) ... & SHIFT_OP
{
    local data:2 = SHIFT_OP;
    local result:2 = data << 1;

    SHIFT_OP = result;
    flag16_NZ(result);
    CF = data[15,1];
}

:BCC ADDR_PC_REL_BRA    is op=0x90; ADDR_PC_REL_BRA
{
    if (CF==0) goto ADDR_PC_REL_BRA;
}

:BCS ADDR_PC_REL_BRA    is op=0xb0; ADDR_PC_REL_BRA
{
    if (CF==1) goto ADDR_PC_REL_BRA;
}

:BEQ ADDR_PC_REL_BRA    is op=0xf0; ADDR_PC_REL_BRA
{
    if (ZF==1) goto ADDR_PC_REL_BRA;
}

BIT_OP_READ8: SEC_OP_X_READ8    is (aaa=1 & cc=0) ... & SEC_OP_X_READ8  { export SEC_OP_X_READ8; }
BIT_OP_READ8: MemImmRead8    is op=0x89; MemImmRead8  { export MemImmRead8; }
BIT_OP_READ16: SEC_OP_X_READ16    is (aaa=1 & cc=0) ... & SEC_OP_X_READ16  { export SEC_OP_X_READ16; }
BIT_OP_READ16: MemImmRead16    is op=0x89; MemImmRead16  { export MemImmRead16; }

# BIT 8 bit.
:BIT BIT_OP_READ8    is (ctx_MF=1 &
    (op=0x89 | op=0x2c | op=0x24 | op=0x3c | op=0x34) & op) ... & BIT_OP_READ8
{
    local data:1 = BIT_OP_READ8;
    local is_immediate:1 = op:1 == 0x89;

    if (is_immediate) goto <skip_flags>;
    flag8_N(data);
    VF = (data & 0x40) != 0;

    <skip_flags>
    local result:1 = A & data;
    flag8_Z(result);
}

# BIT 16 bit.
:BIT BIT_OP_READ16    is (ctx_MF=0 &
    (op=0x89 | op=0x2c | op=0x24 | op=0x3c | op=0x34) & op) ... & BIT_OP_READ16
{
    local data:2 = BIT_OP_READ16;
    local is_immediate:1 = op:1 == 0x89;

    if (is_immediate) goto <skip_flags>;
    flag16_N(data);
    VF = (data & 0x4000) != 0;

    <skip_flags>
    local result:2 = C & data;
    flag16_Z(result);
}

:BMI ADDR_PC_REL_BRA    is op=0x30; ADDR_PC_REL_BRA
{
    if (NF==1) goto ADDR_PC_REL_BRA;
}

:BNE ADDR_PC_REL_BRA    is op=0xd0; ADDR_PC_REL_BRA
{
    if (ZF==0) goto ADDR_PC_REL_BRA;
}

:BPL ADDR_PC_REL_BRA    is op=0x10; ADDR_PC_REL_BRA
{
    if (NF==0) goto ADDR_PC_REL_BRA;
}

:BRA ADDR_PC_REL_BRA    is op=0x80; ADDR_PC_REL_BRA
{
    goto ADDR_PC_REL_BRA;
}

:BRK    is op=0x00
{
    local pc_inc:2 = PC + 2;
    local target:3;
    local is_emulation:1 = $(EF) == 1;
    if (is_emulation) goto <do_emulation>;

    <do_native>
    push8(PBR);
    push16(pc_inc);
    push_status_native();
    IF = 1;
    DF = 0;
    PBR = 0;
    target = zext(*:2 0x00FFE6:3);
    goto <end>;

    <do_emulation>
    push16(pc_inc);
    local save_bf:1 = BF;  BF = 1;  push_status_emulation();  BF = save_bf;
    IF = 1;
    DF = 0;
    target = zext(*:2 0x00FFFE:3);

    <end>
    goto [target];
}

:BRL ADDR_PC_REL_LONG_BRA    is op=0x82; ADDR_PC_REL_LONG_BRA
{
    goto ADDR_PC_REL_LONG_BRA;
}

:BVC ADDR_PC_REL_BRA    is op=0x50; ADDR_PC_REL_BRA
{
    if (VF==0) goto ADDR_PC_REL_BRA;
}

:BVS ADDR_PC_REL_BRA    is op=0x70; ADDR_PC_REL_BRA
{
    if (VF==1) goto ADDR_PC_REL_BRA;
}

:CLC    is op=0x18
{
    CF = 0;
}

:CLD    is op=0xD8
{
    DF = 0;
}

:CLI    is op=0x58
{
    IF = 0;
}

:CLV    is op=0xB8
{
    VF = 0;
}

# CMP 8 bit.
:CMP PRIMARY_OP_READ8    is (ctx_MF=1 &
    (op=0xc9 | op=0xcd | op=0xcf | op=0xc5 | op=0xd2 | op=0xc7 | op=0xdd |
     op=0xdf | op=0xd9 | op=0xd5 | op=0xc1 | op=0xd1 | op=0xd7 | op=0xc3 |
     op=0xd3)) ... & PRIMARY_OP_READ8
{
    local data:1 = PRIMARY_OP_READ8;
    local result:4;

    result = sext(A) - sext(data);
    flag8_NZ(result:1);
    CF = result s>= 0;
}

# CMP 16 bit.
:CMP PRIMARY_OP_READ16    is (ctx_MF=0 &
    (op=0xc9 | op=0xcd | op=0xcf | op=0xc5 | op=0xd2 | op=0xc7 | op=0xdd |
     op=0xdf | op=0xd9 | op=0xd5 | op=0xc1 | op=0xd1 | op=0xd7 | op=0xc3 |
     op=0xd3)) ... & PRIMARY_OP_READ16
{
    local data:2 = PRIMARY_OP_READ16;
    local result:4;

    result = sext(C) - sext(data);
    flag16_NZ(result:2);
    CF = result s>= 0;
}

:COP    is op=0x02
{
    local pc_inc:2 = PC + 2;
    local target:3;
    local is_emulation:1 = $(EF) == 1;
    if (is_emulation) goto <do_emulation>;

    <do_native>
    push8(PBR);
    push16(pc_inc);
    push_status_native();
    IF = 1;
    DF = 0;
    PBR = 0;
    target = zext(*:2 0x00FFE4:3);
    goto <end>;

    <do_emulation>
    push16(pc_inc);
    push_status_emulation();
    IF = 1;
    DF = 0;
    target = zext(*:2 0x00FFF4:3);

    <end>
    goto [target];
}

CPXY_OP_READ8: SEC_OP_COMMON_READ8    is (cc=0) ... & SEC_OP_COMMON_READ8  { export SEC_OP_COMMON_READ8; }
CPXY_OP_READ8: MemImmRead8    is bbb=0; MemImmRead8  { export MemImmRead8; }
CPXY_OP_READ16: SEC_OP_COMMON_READ16    is (cc=0) ... & SEC_OP_COMMON_READ16  { export SEC_OP_COMMON_READ16; }
CPXY_OP_READ16: MemImmRead16    is bbb=0; MemImmRead16  { export MemImmRead16; }

# CPX 8 bit.
:CPX CPXY_OP_READ8    is (ctx_MF=1 & (op=0xe0 | op=0xec | op=0xe4)) ... & CPXY_OP_READ8 {
    local result:4 = sext(XLow) - sext(CPXY_OP_READ8);
    flag8_NZ(result:1);
    CF = result s>= 0;
}

# CPX 16 bit.
:CPX CPXY_OP_READ16    is (ctx_MF=0 & (op=0xe0 | op=0xec | op=0xe4)) ... & CPXY_OP_READ16 {
    local result:4 = sext(X) - sext(CPXY_OP_READ16);
    flag16_NZ(result:2);
    CF = result s>= 0;
}

# CPY 8 bit.
:CPY CPXY_OP_READ8    is (ctx_MF=0 & (op=0xc0 | op=0xcc | op=0xc4)) ... & CPXY_OP_READ8 {
    local result:4 = sext(YLow) - sext(CPXY_OP_READ8);
    flag8_NZ(result:1);
    CF = result s>= 0;
}

# CPY 16 bit.
:CPY CPXY_OP_READ16    is (ctx_MF=1 & (op=0xc0 | op=0xcc | op=0xc4)) ... & CPXY_OP_READ16 {
    local result:4 = sext(Y) - sext(CPXY_OP_READ16);
    flag16_NZ(result:2);
    CF = result s>= 0;
}

DEC_INC_OP: SEC_OP_X    is (cc=2) ... & SEC_OP_X  { export SEC_OP_X; }
DEC_INC_OP: ADDR_ACC    is (bbb=6) ... & ADDR_ACC  { export ADDR_ACC; }

# DEC 8 bit.
