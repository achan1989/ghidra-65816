    DBR = dest_bank;
    local src_addr:3;
    local dest_addr:3;

    <move_loop>
    src_addr = (zext(src_bank) << 16) | zext(X);
    dest_addr = (zext(dest_bank) << 16) | zext(Y);
    *:1 dest_addr = *:1 src_addr;

    X = X + 1;
